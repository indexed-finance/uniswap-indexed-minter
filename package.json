    "buidler-ethers-v5": "^0.2.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.0",
    "decimal.js": "^10.2.1",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^3.2.1",
