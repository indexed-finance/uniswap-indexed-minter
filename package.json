{
  "name": "@indexed-finance/uniswap-indexed-minter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:build": "rimraf ./artifacts && rimraf ./cache && npm run clean:temp",
    "clean:temp": "buidler-verify-helper --clean",
    "prepare": "yarn clean:build && buidler-verify-helper --prepare --contracts IndexedUniswapRouterMinter",
    "prepare:weth": "yarn clean:build && buidler-verify-helper --prepare --contracts MockERC20",
    "prepare:liquidity": "yarn clean:build && buidler-verify-helper --prepare --contracts LiquidityAdder",
    "deploy:rinkeby": "yarn prepare && buidler --network rinkeby deploy",
    "deploy:mainnet": "yarn prepare && buidler --network mainnet deploy",
    "deploy:weth": "yarn prepare:weth && buidler --network rinkeby deploy",
    "deploy:liquidity": "yarn prepare:liquidity && buidler --network rinkeby deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
